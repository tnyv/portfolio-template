  -webkit-animation: load 1s linear 1.5s 1 forwards, changez 1s linear 2s 1 forwards;
          animation: load 1s linear 1.5s 1 forwards, changez 1s linear 2s 1 forwards;
}

@-webkit-keyframes load {
  from {
    opacity: 1;
  }
  to {
    opacity: 0;
  }
}

@keyframes load {
  from {
    opacity: 1;
  }
  to {
    opacity: 0;
  }
}

@-webkit-keyframes changez {
  to {
